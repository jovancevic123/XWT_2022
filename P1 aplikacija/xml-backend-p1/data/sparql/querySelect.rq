PREFIX schema: <http://www.ftn.uns.ac.rs/rdf/examples/predicate>
prefix xsd: <http://www.w3.org/2001/XMLSchema#>
SELECT * from <http://localhost:3030/PatentDataset/data/patenti/metadata>
WHERE {
  ?patent  <http://www.ftn.uns.ac.rs/rdf/examples/predicate/broj_prijave> ?brojPrijave
  FILTER(CONTAINS(UCASE(str(?brojPrijave)), UCASE("%1$s"))).
}